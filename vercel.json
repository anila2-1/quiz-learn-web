{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "^/api/payload/(.*)$",
      "dest": "/api/payload/$1"
    },
    {
      "src": "^/admin/(.*)$",
      "dest": "/admin/$1"
    },
    {
      "src": "^/admin$",
      "dest": "/admin/"
    }
  ]
}
